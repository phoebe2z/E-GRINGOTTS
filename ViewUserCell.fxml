<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane id="AnchorPane" fx:id="Base" prefHeight="40.0" prefWidth="364.0" styleClass="CellBase" stylesheets="@../CSSClass/ViewUserCell.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="egringgots.ViewUserCellController">
   <children>
      <HBox fx:id="CellHbox" prefHeight="40.0" prefWidth="364.0">
         <children>
            <ImageView fx:id="UserPfp" fitHeight="36.0" fitWidth="36.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Image/DefaultPFP.png" />
               </image>
               <HBox.margin>
                  <Insets left="5.0" top="2.0" />
               </HBox.margin>
            </ImageView>
            <Label fx:id="NameLabel" prefHeight="17.0" prefWidth="82.0" styleClass="Basic_text" text="Damien" textAlignment="CENTER" textFill="#0c690a">
               <HBox.margin>
                  <Insets left="20.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Label fx:id="NoAccLabel" layoutX="70.0" layoutY="20.0" prefHeight="14.0" prefWidth="93.0" styleClass="Basic_text" text="| 2222 2222 2222">
               <HBox.margin>
                  <Insets left="5.0" top="10.0" />
               </HBox.margin>
            </Label>
            <Button fx:id="ViewBtn" mnemonicParsing="false" onAction="#View_Btn" prefHeight="25.0" prefWidth="53.0" styleClass="button_regular" text="View ">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="DeleteBtn" layoutX="226.0" layoutY="15.0" mnemonicParsing="false" onAction="#Delete_Btn" prefHeight="25.0" prefWidth="29.0">
               <HBox.margin>
                  <Insets left="10.0" top="5.0" />
               </HBox.margin>
               <graphic>
                  <ImageView fitHeight="18.0" fitWidth="83.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../Image/delete.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
   </children>
</AnchorPane>
